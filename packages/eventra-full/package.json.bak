{
  "name": "eventra-saas",
  "version": "0.1.0",
  "private": true,
  "description": "A modern event management platform built with Next.js 15, TypeScript, and Prisma",
  "author": "Your Name",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "typescript",
    "event-management",
    "saas",
    "prisma",
    "tailwindcss"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:deploy": "prisma migrate deploy",
    "db:setup": "npm run db:deploy && npm run db:seed",
    "db:studio": "prisma studio",
    "clean": "rimraf .next out",
    "i18n:check": "node scripts/validate-i18n.js",
    "import:events:csv": "tsx scripts/csv-to-events-json.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:all": "node scripts/test-all.js",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed --project=chromium",
    "playwright:install": "playwright install",
    "prebuild": "echo skipping type-check and lint for build",
    "postinstall": "prisma generate",
    "dev:check": "npm run type-check && npm run lint && npm run i18n:check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pre-commit": "lint-staged",
    "setup:dev": "npm install && npx husky init && npm run db:setup",
    "setup:docker": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "csv-parse": "^6.1.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.9",
    "next-pwa": "^5.6.0",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axe-core": "^4.10.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.11.3",
    "node-mocks-http": "^1.17.2",
    "playwright": "^1.55.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "bash -c 'npm run type-check'"
    ]
  }
}
